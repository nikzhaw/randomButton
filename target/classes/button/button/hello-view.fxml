<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="button.button.ButtonController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ToolBar prefHeight="40.0" prefWidth="200.0">
     <items>
         <Label text="ToDo" />
         <TextField id="newAnswer" fx:id="newAnswer" />
       <Button id="addAnswer" fx:id="addButton" alignment="CENTER_RIGHT" contentDisplay="RIGHT" disable="true" mnemonicParsing="false" onAction="#addAnswer" text="Add" />
     </items>
   </ToolBar>

    <Label fx:id="welcomeText" />
    <Button id="toDoButton" fx:id="toDoButton" onAction="#onToDocklick" text="Do something">
      <font>
         <Font size="24.0" />
      </font></Button>
</VBox>
